@maxwidth = 1000

window = {
	gfxtype = windowgfx
	name = "government_view"
	
	parentanchor = top|left
	size = { @maxwidth 900 }
	position = { 5 120 }
	widgetanchor = top|left
	movable = no

	using = advice_trigger_highlights
	
	widget = {
		name = "background"
		using = Large_window_type
		size = { 100% 100% }
		alwaystransparent = yes
		
		blockoverride "header"
		{
			text = "GOVERNMENT_MENU_HEADER"
			tooltip = "GOVERNMENT_MENU_HEADER_TT"
		}
	}
	
	
	square_close_button = {
		name = "close_button"
		position = { -12 12 }
		parentanchor = top|right
		shortcut = "close_window"
		onclick = "[GovernmentView.OnClose]"
		tooltip = "CLOSE"
	}
	
	left_arrow_button = {
		name = "previous_button"
		position = { 294 14 }
		parentanchor = top|left
		shortcut = "previous_window"
		onclick = "[GovernmentView.OnPrevious]"
		tooltip = "PREVIOUS_GOVERNMENT_VIEW"
	}
		
	### Tab Buttons
	widget = {
		name = "tab_buttons"
		size = { 100% 40 }
		position = { 0 70 }

		flowContainer = {
			direction = horizontal
			parentanchor = top|hcenter
			widgetanchor = top|hcenter
			spacing = 10

			sort_button = {
				name = "show_government"
				visible = yes
				size = { 280 40 }
				tooltip = GOV_VIEW_GOV_TOOLTIP
				onclick = "[GovernmentView.ShowGovernment]"
				down = "[GovernmentView.IsShowGovernment]"
				
				textbox = {
					using = MarbleButtonFontM
					#parentanchor = top|left
					size = { 280 24 }
					position = { 6 4 }
					align = center
					text = "SHOW_GOVERNMENT"
				}
			}
	
			sort_button = {
				name = "show_offices"
				visible = yes
				
				size = { 280 40 }
				tooltip = GOV_VIEW_OFFICES_TOOLTIP
				onclick = "[GovernmentView.ShowOffices]"
				down = "[GovernmentView.IsShowOffices]"
				
				textbox = {
					using = MarbleButtonFontM
					#parentanchor = top|left
					size = { 280 24 }
					position = { 6 4 }
					align = center
					text = "SHOW_OFFICES"
				}
			}

			sort_button = {
				name = "show_laws"
				visible = yes
				
				size = { 280 40 }
				tooltip = GOV_VIEW_LAWS_TOOLTIP
				onclick = "[GovernmentView.ShowLaws]"
				down = "[GovernmentView.IsShowLaws]"
				
				textbox = {
					using = MarbleButtonFontM
					#parentanchor = top|left
					size = { 280 24 }
					position = { 6 4 }
					align = center
					text = "SHOW_LAWS"
				}
			}
		}
	}
	
	
